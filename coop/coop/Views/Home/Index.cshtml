@model List< coop.Models.member >


@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div id="listOfGames">


    <table class="table table-striped">
        <thead>
            <tr>
                <th class="col-sm-2">Name</th>
                <th class="col-sm-6">City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
                <tr>
                    <td>@item.name</td>
                    <td>@item.city</td>
                    
                </tr>
            }

        </tbody>
    </table>

</div>


<div>
    <a href="#" class="btn btn-info" role="button">Modify</a>
    <a href="#" class="btn btn-info" role="button">Create</a>

</div>